<%- include('../adminHeader.ejs') %>

<div class="card text-center" style="  position: absolute;
left: 50px;
width: calc(100% - 100px);
margin:10px;
padding: 5px;
text-align: center;">
  <div class="card-body">
    <h4 class="card-title">Card title</h4>
    <h5 class="card-subtitle mb-2 text-muted"><%= view.title %></h5>
    <h4 class="card-title">Description</h4>
    <p class="card-text"><%= view.description %></p>
    <h4 class="card-title">Image</h4>
 <img src="<%= view.image.url %>" alt="" srcset="">
  </div>
</div>

<%- include('../adminFooter.ejs') %>
